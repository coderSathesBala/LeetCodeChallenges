/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    for (i=0; nums.length; i++) {
        let numero=nums[i] 
            for(j=1; j<=nums.length; j++) {
                if(numero + nums[j] === target && j !== i) {
                    return [i, j]
                }
            }
        
    } 
};
